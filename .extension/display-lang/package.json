{
    "name": "display",
    "displayName": "display",
    "description": "Display (XML) File",
    "publisher": "ender",
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.88.0"
    },
    "scripts": {
        "publish": "vsce publish",
        "build": "vsce package"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "display-xml",
                "aliases": [
                    "DISPLAY",
                    "display-xml"
                ],
                "extensions": [
                    ".display"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "display-xml",
                "scopeName": "source.display.xml",
                "path": "./syntaxes/display-xml.tmLanguage.json"
            }
        ],
        "snippets": [
            {
              "language": "display-xml",
              "path": "./snippets/display.code-snippets"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Ender-ing/render-activity.git"
    }
}